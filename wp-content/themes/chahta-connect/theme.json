{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": true,
		"color": {
			"custom": false,
			"customDuotone": false,
			"customGradient": false,
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [],
			"gradients": [],
			"palette": [
				{
					"color": "#00558C",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#BFD5E2",
					"name": "Primary Light",
					"slug": "primary-light"
				},
				{
					"color": "#002238",
					"name": "Primary Dark",
					"slug": "primary-dark"
				},
				{
					"color": "#B5E3D8",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#EDF8F5",
					"name": "Secondary Light",
					"slug": "secondary-light"
				},
				{
					"color": "#485B56",
					"name": "Secondary Dark",
					"slug": "secondary-dark"
				},
				{
					"color": "#E04F39",
					"name": "Accent",
					"slug": "accent"
				},
				{
					"color": "#5A2017",
					"name": "Accent Dark",
					"slug": "accent-dark"
				},
				{
					"color": "#4D6474",
					"name": "Card Price Background",
					"slug": "card-price-bg-color"
				},
				{
					"color": "#C02B0A",
					"name": "Form Danger",
					"slug": "form-danger"
				},
				{
					"color": "#000E16",
					"name": "Dark",
					"slug": "dark"
				},
				{
					"color": "#959391",
					"name": "Gray",
					"slug": "gray"
				},
				{
					"color": "#FBFDFF",
					"name": "Light",
					"slug": "light"
				}
			]
		},
		"blocks": {
			"core/group": {
				"background": {
					"backgroundImage": true,
					"backgroundSize": true
				}
			}
		},
		"dimensions": {
			"aspectRatio": true
		},
		"layout": {
			"allowCustomContentAndWideSize": false,
			"allowEditing": true,
			"wideSize": "1320px",
			"contentSize": "768px"
		},
		"shadow": {
			"defaultPresets": false
		},
		"spacing": {
			"blockGap": true,
			"customSpacingSize": false,
			"defaultSpacingSizes": false,
			"margin": true,
			"padding": true,
			"spacingSizes": [
				{
					"name": "None",
					"size": "0",
					"slug": "none"
				},
				{
					"name": "Extra Small",
					"size": ".25rem",
					"slug": "xs"
				},
				{
					"name": "Small",
					"size": ".5rem",
					"slug": "sm"
				},
				{
					"name": "Base",
					"size": "1rem",
					"slug": "base"
				},
				{
					"name": "Large",
					"size": "1.5rem",
					"slug": "lg"
				},
				{
					"name": "Extra Large",
					"size": "3rem",
					"slug": "xl"
				}
			]
		},
		"typography": {
			"customFontSize": false,
			"defaultFontSizes": false,
			"dropCap": false,
			"fontFamilies": [
				{
					"fontFamily": "gill-sans-nova, \"Helvetica Neue\", \"Noto Sans\", \"Liberation Sans\", Arial, sans-serif, \"Apple Color Emoji\", \"Segoe UI Emoji\", \"Segoe UI Symbol\", \"Noto Color Emoji\"",
					"name": "Gill Sans Nova",
					"slug": "body"
				},
				{
					"fontFamily": "pill-gothic-300mg, \"Helvetica Neue\", \"Noto Sans\", \"Liberation Sans\", Arial, sans-serif, \"Apple Color Emoji\", \"Segoe UI Emoji\", \"Segoe UI Symbol\", \"Noto Color Emoji\"",
					"name": "Pill Gothic 300mg",
					"slug": "headings"
				}
			],
			"fontSizes": [
				{
					"slug": "root",
					"size": "1rem",
					"name": "Root",
					"fluid": {
						"min": "1rem",
						"max": "1.25rem"
					}
				},
				{
					"slug": "base",
					"size": "1.35rem",
					"name": "Body",
					"fluid": false
				},
				{
					"slug": "sm",
					"size": "1.2rem",
					"name": "Small (H6)",
					"fluid": {
						"min": "1.2rem",
						"max": "1.5rem"
					}
				},
				{
					"slug": "md",
					"size": "1.44rem",
					"name": "Medium (H5)",
					"fluid": {
						"min": "1.44rem",
						"max": "1.88rem"
					}
				},
				{
					"slug": "lg",
					"size": "1.728rem",
					"name": "Large (H4)",
					"fluid": {
						"min": "1.728rem",
						"max": "2.16rem"
					}
				},
				{
					"slug": "xl",
					"size": "2.074rem",
					"name": "XL (H3)",
					"fluid": {
						"min": "2.074rem",
						"max": "2.592rem"
					}
				},
				{
					"slug": "xxl",
					"size": "2.488rem",
					"name": "2XL (H2)",
					"fluid": {
						"min": "2.488rem",
						"max": "3.11rem"
					}
				},
				{
					"slug": "xxxl",
					"size": "2.986rem",
					"name": "3XL (H1)",
					"fluid": {
						"min": "2.986rem",
						"max": "3.73rem"
					}
				}
			],
			"fontWeight": true,
			"lineHeight": false,
			"textDecoration": false
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"spacing": {
			"padding": {
				"bottom": "var:preset|spacing|none",
				"left": "calc(var(--wp--preset--spacing--lg) * .5)",
				"right": "calc(var(--wp--preset--spacing--lg) * .5)",
				"top": "var:preset|spacing|none"
			},
			"blockGap": "var:preset|spacing|base"
		},
		"blocks": {
			"core/columns": {
				"spacing": {
					"blockGap": "var:preset|spacing|base"
				}
			},
			"core/heading": {
				"typography": {
					"fontFamily": "var:preset|font-family|headings"
				},
				"spacing": {
					"margin": {
						"bottom": "var:preset|spacing|sm",
						"top": "var:preset|spacing|none"
					}
				}
			},
			"core/list": {
				"typography": {
					"fontFamily": "var:preset|font-family|body",
					"fontSize": "var:preset|font-size|base"
				}
			},
			"core/list-item": {
				"typography": {
					"fontFamily": "var:preset|font-family|body",
					"fontSize": "var:preset|font-size|base"
				}
			},
			"core/paragraph": {
				"typography": {
					"fontFamily": "var:preset|font-family|body",
					"fontSize": "var:preset|font-size|base"
				},
				"spacing": {
					"margin": {
						"bottom": "var:preset|spacing|base",
						"top": "var:preset|spacing|none"
					}
				}
			},
			"core/cover": {
				"spacing": {
					"padding": {
						"top": "var:preset|spacing|none",
						"right": "var:preset|spacing|none",
						"bottom": "var:preset|spacing|none",
						"left": "var:preset|spacing|none"
					},
					"blockGap": "var:preset|spacing|base"
				}
			},
			"core/group": {
				"spacing": {
					"blockGap": "var:preset|spacing|base"
				}
			},
			"core/button": {
				"border": {
					"radius": "0",
					"width": "2px",
					"color": "var:preset|color|primary"
				},
				"spacing": {
					"padding": {
						"top": "var:preset|spacing|sm",
						"right": "var:preset|spacing|lg",
						"bottom": "var:preset|spacing|sm",
						"left": "var:preset|spacing|lg"
					},
					"blockGap": "var:preset|spacing|base"
				},
				"typography": {
					"fontFamily": "var:preset|font-family|body",
					"fontSize": "var:preset|font-size|base"
				},
				"color": {
					"background": "var:preset|color|primary",
					"text": "var:preset|color|white"
				}
			}
		},
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "var:preset|font-size|xxxl"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var:preset|font-size|xxl"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var:preset|font-size|xl"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var:preset|font-size|lg"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var:preset|font-size|md"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var:preset|font-size|sm"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var:preset|font-family|headings"
				},
				"spacing": {
					"margin": {
						"bottom": "var:preset|spacing|sm",
						"top": "0"
					}
				},
				"css": "text-wrap:pretty"
			},
			"link": {
				"typography": {
					"fontSize": "var:preset|font-size|base"
				},
				"color": {
					"text": "var:preset|color|secondary"
				}
			}
		},
		"typography": {
			"fontSize": "var:preset|font-size|base"
		}
	}
}
